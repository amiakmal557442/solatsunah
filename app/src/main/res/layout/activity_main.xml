<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tahajjud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="salat_tahajjud"
        android:textSize="20sp"
        android:layout_marginTop="100dp"/>
    <TextView
        android:id="@+id/duha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="solat duha"
        android:textSize="20sp"
        android:layout_marginTop="100dp"/>
    <TextView
        android:id="@+id/rawatib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="salat rawatib"
        android:textSize="20sp"
        android:layout_marginTop="100dp"/>
    <TextView
        android:id="@+id/istikharah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="salat istikharah"
        android:textSize="20sp"
        android:layout_marginTop="100dp"/>
    <TextView
        android:id="@+id/hajad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="salat hajad"
        android:textSize="20sp"
        android:layout_marginTop="100dp"/>
    <TextView
        android:id="@+id/tahiatul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="salat tahiatul masjid"
        android:textSize="20sp"
        android:layout_marginTop="100dp"/>
    <TextView
        android:id="@+id/syuruq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="salat syuruq"
        android:textSize="20sp"
        android:layout_marginTop="100dp"/>
    <TextView
        android:id="@+id/witir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="salat witir"
        android:textSize="20sp"
        android:layout_marginTop="100dp"/>







</LinearLayout>